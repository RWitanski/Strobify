<Window x:Class="Strobify.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        xmlns:vm="clr-namespace:Strobify.ViewModel"
        DataContext="{DynamicResource MainViewModel}"
        Title="MainWindow" Height="350" Width="525">    
    <Window.Resources>
        <vm:MainViewModel x:Key="MainViewModel"/>        
    </Window.Resources>
    <Grid>
        <Button Content="{DynamicResource MainViewModel}" HorizontalAlignment="Left" Margin="10,289,0,0" VerticalAlignment="Top" Width="100" Command="{Binding Mode=OneWay}"/>
    </Grid>
</Window>
